# Database
DATABASE_URL=sqlite:///./data/sync.db

# Azure AD (Microsoft 365 Business)
AZURE_CLIENT_ID=your-azure-client-id
AZURE_CLIENT_SECRET=your-azure-client-secret
AZURE_TENANT_ID=your-tenant-id
AZURE_REDIRECT_URI=https://sync.lincsolution.net/auth/callback/microsoft

# Google Workspace
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=https://sync.lincsolution.net/auth/callback/google
GOOGLE_WORKSPACE_DOMAIN=lincsolution.net

# Security
SECRET_KEY=your-secret-key-for-jwt-min-32-chars
ENCRYPTION_KEY=your-encryption-key-for-tokens-44-chars

# Webhook
WEBHOOK_BASE_URL=https://sync.lincsolution.net

# Application
LOG_LEVEL=INFO
